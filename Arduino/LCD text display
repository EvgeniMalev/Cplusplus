#include <LiquidCrystal.h>
const int rs = 12, en = 11, d4 = 5, d5 = 4, d6 = 3, d7 = 2;
LiquidCrystal lcd(rs, en, d4, d5, d6, d7);

void setup() {
  lcd.begin(16, 2);
  lcd.print("Good day!");
  delay(1000); 
  lcd.clear(); 
  lcd.print("Welcome to SOF!");
    delay(3000); 
  lcd.clear(); 
  lcd.print("23.02.2023");
}

